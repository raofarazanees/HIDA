{
  "name": "hida-ui",
  "version": "1.13.0",
  "description": "Hida Workbench Mastering platform",
  "private": true,
  "main": "src/index.ts",
  "bin": {
    "seed-application": "./bin/install.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.clarivate.io/hida/hida-ui.git"
