  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dest && rimraf ./reports",
    "lint": "eslint \"*.js\" \"./server/*.js\" && ng lint",
    "ci": "yarn lint && yarn test --no-watch --code-coverage && yarn build && copy ./server/*.js ./dest",
    "debug": "npm -v && yarn -v && node -v",
    "build": "ng build --extra-webpack-config webpack.partial.js --configuration production --deploy-url",
    "develop": "ng serve --hmr --proxy-config proxy.conf.json",
    "release": "yarn clean && yarn build && node cdnize-build.js && copy ./server/*.js ./dest",
    "nodejs": "copy ./server/*.js ./dest && copy ./app.config.js ./dest/server && CLOUD_APP=cdxseed DEBUG=* node ./dest/server/server-runner.js",
    "serve": "export LOCAL_SERVE=true && yarn build && yarn nodejs",
    "start": "yarn develop",
    "test": "yarn clean && ng test",
    "e2e": "ng e2e"
  },
